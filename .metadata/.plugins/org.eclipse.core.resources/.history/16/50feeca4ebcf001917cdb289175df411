package test1;

public class 변환_2진수_10진수2 {

	public static void main(String[] args) {
		int t[]= {0,1,1,1,0,0,0}; //2진수
		int d=0;
		if(t[0]==0) {
			for(int i=0;i<t.length;i++) {
				d+=t[i]*(int) Math.pow(2, t.length-i);
				
			}
			System.out.println(d);
		}
		for(int i=0;i<t.length;i++) {
			
			
		}
	}

}
